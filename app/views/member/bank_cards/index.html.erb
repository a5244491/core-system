<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <%= render partial: 'member/individual_credit_accounts/individual_credit_account_show_header' %>
            <div class="panel-body">
                <%= render partial: 'member/individual_credit_accounts/individual_credit_account_show_tab_header', locals: {index: 1} %>
                <div class="panel">
                    <div class="panel-body">
                            <table class="table table-striped table-bordered table-hover">
                                <thead>
                                <tr>
                                    <th>银行卡号</th>
                                    <th>卡片类型</th>
                                    <th>银行名称</th>
                                    <th>获得积分</th>
                                    <th>绑定时间</th>
                                </tr>
                                </thead>
                                <tbody>
                                  <%= render partial: 'bank_card', collection: @member_bank_cards, as: :bank_card %>
                                </tbody>
                            </table>
                     </div>
                </div>
            </div>
            <!-- /.panel-body -->
        </div>
        <!-- /.panel -->
    </div>
    <!-- /.col-lg-12 -->
</div>

