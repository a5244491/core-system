<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <%= render partial: 'member/individual_credit_accounts/individual_credit_account_show_header' %>
            <div class="panel-body">
                <%= render partial: 'member/individual_credit_accounts/individual_credit_account_show_tab_header', locals: {index: 2} %>
                <%= render 'search_form' %>
                <div class="panel">
                    <div class="panel-body">
                        <div class="row">
                            <%= render partial: 'shared/index_table_entries_info', locals: {collection: @member_vouchers, model: 'voucher'} %>
                        </div>
                        <br>
                        <table class="table table-striped table-bordered table-hover">
                            <thead>
                            <tr>
                                <th>红包别名</th>
                                <th>领取时间</th>
                                <th>使用时间</th>
                                <th>面额</th>
                                <th>类别</th>
                                <th>状态</th>
                            </tr>
                            </thead>
                            <tbody>
                               <%= render partial: 'voucher', collection: @member_vouchers, as: :voucher %>
                            </tbody>
                        </table>
                        <div class="row">
                            <%= render partial: 'shared/index_table_entries_info', locals: {collection: @member_vouchers, model: 'voucher'} %>
                        </div>
                    </div>
                </div>
            </div>
            <!-- /.panel-body -->
        </div>
        <!-- /.panel -->
    </div>
    <!-- /.col-lg-12 -->
</div>

