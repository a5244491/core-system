<%= form_for(@merchant_acquirer_org, html: {class: 'form-horizontal', role: 'form', id: 'acquirer_form'}) do |f| %>
    <div class="form-group">
        <%= f.label :acquirer_name, '收单机构名称', class: 'col-sm-2 control-label' %>
        <div class="col-xs-6">
            <%= f.text_field(:acquirer_name, class: 'form-control', 'data-bv-notempty' => true) %>
        </div>
    </div>

    <div class="form-group">
        <%= f.label :acquirer_code, '收单机构代码', class: 'col-sm-2 control-label' %>
        <div class="col-xs-6">
            <%= f.text_field(:acquirer_code, class: 'form-control', 'data-bv-notempty' => true) %>
        </div>
    </div>
    <div class="form_footer">
        <a class="btn btn-default" href="<%=merchant_acquirer_orgs_path %>">返回</a>
        <button type="reset" class="btn btn-danger">重置</button>
        <button type="submit" class="btn btn-success"><i class="fa fa-check"></i> 提交</button>
    </div>
<% end %>
