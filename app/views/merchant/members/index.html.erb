<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <%= render 'merchant/merchant_stores/merchant_store_show_header' %>
            <!-- /.panel-heading -->
            <div class="panel-body">
                <%= render partial: 'merchant/merchant_stores/merchant_show_tab_header', locals: {index: 3} %>
                <%= render 'search_form' %>
                <br>
                <div class="row">
                    <%= render partial: 'shared/index_table_entries_info', locals: {collection: @member_accounts, model: 'member'} %>
                </div>
                <br>
                <div class="table-responsive">
                    <table class="table table-striped table-bordered table-hover">
                        <thead>
                        <tr>
                            <th>手机号</th>
                            <th>注册时间</th>
                            <th>消费次数</th>
                            <th>可用积分</th>
                        </tr>
                        </thead>
                        <tbody>
                        <%= render partial: 'member', collection: @member_accounts, as: :member_account %>
                        </tbody>
                    </table>
                </div>
                <div class="row">
                    <%= render partial: 'shared/index_table_entries_info', locals: {collection: @member_accounts, model: 'member'} %>
                </div>
            </div>
            <!-- /.panel-body -->
        </div>
        <!-- /.panel -->
    </div>
    <!-- /.col-lg-12 -->
</div>

